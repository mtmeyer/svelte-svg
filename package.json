{
  "name": "svg-svelte",
  "version": "0.1.0",
  "description": "A simple CLI tool to generate Svelte components from SVG files",
  "main": "index.js",
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "svg-svelte": "node ./lib/bundle.js"
  },
  "bin": {
    "svg-svelte": "bin/svg-svelte"
  },
  "keywords": [
    "cli",
    "svelte",
    "svg"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mtmeyer/svg-svelte.git"
  },
  "bugs": {
    "url": "https://github.com/mtmeyer/svg-svelte/issues"
  },
  "homepage": "https://github.com/mtmeyer/svg-svelte#readme",
  "author": "Michael Meyer",
  "license": "MIT",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^20.0.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@types/jsdom": "^16.2.13",
    "@types/prettier": "^2.3.2",
    "@types/svgo": "^2.3.1",
    "arg": "^5.0.0",
    "chalk": "^4.1.2",
    "inquirer": "^8.1.2",
    "rollup": "^2.55.1",
    "rollup-plugin-auto-external": "^2.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript": "^1.0.1",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.10",
    "esm": "^3.2.25",
    "prettier": "^2.3.2",
    "svgo": "^2.3.1",
    "template-file": "^5.1.0"
  }
}
